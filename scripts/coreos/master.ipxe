#!ipxe

echo CoreOS Master
dhcp net0 && route
set base-url http://192.168.130.10:81
set coreos-version rhcos-4.3.0-x86_64
kernel ${base-url}/${coreos-version}-installer-kernel
initrd ${base-url}/${coreos-version}-installer-initramfs.img
imgargs ${coreos-version}-installer-kernel coreos.inst=yes coreos.inst.install_dev=vda coreos.inst.image_url=${base-url}/${coreos-version}-metal.raw.gz coreos.inst.ignition_url=${base-url}/master.ign rd.neednet=1
boot
